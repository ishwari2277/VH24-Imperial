        <script>
            document.querySelectorAll('.faq-question').forEach((question) => {
    question.addEventListener('click', () => {
        // Get the corresponding answer
        const answer = question.nextElementSibling;

        // Toggle the visibility of the answer
        if (answer.style.display === "block") {
            answer.style.display = "none"; // Collapse the answer
            question.querySelector('.icon').textContent = "+"; // Change icon to plus
        } else {
            answer.style.display = "block"; // Expand the answer
            question.querySelector('.icon').textContent = "–"; // Change icon to minus
        }
    });
});

